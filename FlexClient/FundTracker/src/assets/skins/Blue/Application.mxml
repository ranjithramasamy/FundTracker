<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/halo">
		<fx:Metadata>
			  [HostComponent("spark.components.Application")]		
		</fx:Metadata>
		<s:states>
			<s:State name="normal"/>
			<s:State name="disabled"/>
		</s:states>
		<s:Rect width="{hostComponent.width}"
			height="{hostComponent.height}">
	
		<s:fill>
			<s:RadialGradient>
				<s:GradientEntry color="0x1b445c"/>
				<s:GradientEntry color="0x143348"/>
			</s:RadialGradient>
		</s:fill>
		<s:filters>
			<s:GlowFilter blurX="500" blurY="500" quality="10" />
		</s:filters>
	</s:Rect>
		<s:Group id="contentGroup"
			 left="0"
			 right="0"
			 top="0"
			 bottom="0">
		<s:layout>
			<s:BasicLayout/>
		</s:layout>
	</s:Group>
</s:Skin>
